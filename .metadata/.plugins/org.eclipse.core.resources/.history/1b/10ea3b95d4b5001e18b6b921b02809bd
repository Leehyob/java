package ex02;

public class Example02 {

	public static void main(String[] args) {
		int x = 10, y = 10;
		System.out.println(x == 10);
		System.out.println("x와 y는 같습니까?" + (x == y));
		
		int a = 10, b = 25;
		System.out.println("a == b" + (a == b));
		System.out.println("a != b" + (a != b));
		System.out.println("a > b" + (a > b));
		System.out.println("a < b" + (a < b));
		System.out.println("b >= a" + (b >= a));
		System.out.println("b <= a" + (b <= a));
		

	}

}
