package ex02;

import java.util.Scanner;

public class Exam02 {

	public static void main(String[] args) {
		//1~100 사이에 3의 배수이면서 5의 배수 합 구하고, 갯수 구하기
	
		Scanner sc = new Scanner(System.in);
		
		System.out.println("다섯 개의 숫자를 입력하세요");
		
		int sum = 0;
		
		for(int i=1; i<=5; i++) {
			int num = sc.nextInt();
			sum += num + i;
		}
		
		System.out.println(sum);

	}

}
