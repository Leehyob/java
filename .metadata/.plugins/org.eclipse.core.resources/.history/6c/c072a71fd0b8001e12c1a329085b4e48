package ex01;

public class Switch02 {

	public static void main(String[] args) {
		int grade = 90;
		
		
		switch (grade/10) {
		case 'A' :
			System.out.println("A");
			break;
		case 'B' :
			System.out.println("B");			
			break;
		case 'C' :
			System.out.println("C");
			break;
		case 'D' :
			System.out.println("D");
			break;
		default :
			System.out.println("F");
			break;
		}

	}

}
