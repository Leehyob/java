package ex2;

import java.util.Scanner;

public class Exam01 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("숫자 입력 : ");
		int num = sc.nextInt();
		
		if (num%15 == 0) {
			System.out.println("좋아요");
		}
		else {
			System.out.println("싫어요");
		}

	}

}
