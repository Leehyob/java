package ex02;

import java.util.Scanner;

public class Exam02 {

	public static void main(String[] args) {
		//1~100 사이에 3의 배수이면서 5의 배수 합 구하고, 갯수 구하기
	
		Scanner sc = new Scanner(System.in);
		
		int even=0;
		int odd=0;
		
		for (int i=1; i<=100; i++) {
			
			if (i%2==0) {
				even += i;
				System.out.println(even);
			}else {
				odd+=i;
				System.out.println(odd);
			}
		}
	}

}
