package ex01;

public class InputOutput01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String myName = "홍길동";
		int myAge = 20;
		
		System.out.println("이름 : \n" + myName);
		System.out.println("나이 : " + myAge);
		System.out.println("JavaStudy\'s");
		
		System.out.println("나누기 : " + 5.0/2.0);
		System.out.println("나머지 : " + 5%3);
		
		

	}

}
