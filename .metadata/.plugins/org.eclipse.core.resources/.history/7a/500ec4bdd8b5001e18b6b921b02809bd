package ex02;

public class Operator01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a= 10;
		double b = 2.0;
		System.out.println("a + b =" + (a + b));
		System.out.println("a - b =" + (a - b));
		System.out.println("a * b =" + (a * b));
		System.out.println("a / b =" + (a / b));
		System.out.println("a % b =" + (a % b));

		int c = 10;
		int d = 20;
		int e;
		System.out.println(e = c);
		System.out.println(d += c);
		System.out.println(d -= c);
		System.out.println(d *= c);
		System.out.println(d %= c);
		
		
	}

}
