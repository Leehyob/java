package ex02;


public class Exam02 {

	public static void main(String[] args) {
		//1~100 사이에 3의 배수이면서 5의 배수 합 구하고, 갯수 구하기
		
		int sum = 0;

	
		for(int i=1; i<=100; i++) {
			if (i%3==0 && i%5==0) {
				sum += i;
			
		}else {
			System.out.println("3과 5의 배수가 아닙니다.");
		}
		}
		System.out.println(sum);
		
	}	

}
