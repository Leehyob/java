package ex1;

import java.util.Scanner;

public class conversion01 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		System.out.println("나이를 입력하십시오 : ");
		int a = input.nextInt();
		
		if(a>19) {
			System.out.println("성년입니다.");
		}
		else
			{System.out.println("미성년입니다.");
			
			}

	}

}
