package ex01;

public class Switch02 {

	public static void main(String[] args) {
		int grade = 90;
		
		
		switch (grade/10) {
		case 9 :
			System.out.println("A");
		case 8 :
			System.out.println("B");			
		case 7 :
			System.out.println("C");
		case 6 :
			System.out.println("D");
		default :
			System.out.println("F");
			break;
		}

	}

}
